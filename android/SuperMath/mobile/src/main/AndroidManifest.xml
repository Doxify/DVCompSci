<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.supermath.jacobgb24.supermath" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Integral"
            android:label="Integral"
            android:parentActivityName="com.supermath.jacobgb24.supermath.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.supermath.jacobgb24.supermath.MainActivity" />
            </activity>

        <activity android:name=".AreaFinder"
            android:label="Area Finder"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.supermath.jacobgb24.supermath.MainActivity" />
            </activity>
        <activity android:name=".Kinematics"
            android:label="Kinematics"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.supermath.jacobgb24.supermath.MainActivity" />
        </activity>
        <activity android:name=".Roots"
            android:label="Quadratic Solver"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.supermath.jacobgb24.supermath.MainActivity" />
        </activity>
        <activity android:name=".SettingsActivity"
            android:label="Settings"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.supermath.jacobgb24.supermath.MainActivity" />
        </activity>
    </application>

</manifest>
